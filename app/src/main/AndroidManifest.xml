<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.commonframe"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoadingActivity"
            android:theme="@style/LoadingTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>

                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>

                <data
                    android:host="example.com"
                    android:pathPrefix="/someresource/"
                    android:scheme="gybs"/>
                <data
                    android:host="www.example.com"
                    android:pathPrefix="/someresource/"
                    android:scheme="gybs"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".greendao.GreenDaoDemoActivity"
            android:theme="@style/AppTheme"/>
        <activity
            android:name=".glide.GlideDemoActivity"
            android:theme="@style/AppTheme"/>
        <activity
            android:name=".mvp.ui.activity.MainActivity"
            android:theme="@style/AppTheme"/>
        <activity
            android:name=".scroller.ScrollerActivity"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".IndexActivity"
            android:theme="@style/AppTheme"/>
        <activity
            android:name=".material.MaterialActivity"
            android:theme="@style/AppTheme"/>
        <activity
            android:name=".fragment.FragmentActivity"
            android:theme="@style/AppTheme"/>
        <activity
            android:name=".glide.PhotoActivity"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".baseview.BaseViewActivity"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".baseview.WebActivity"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".recyclerview.RecyclerViewDemoActivity"
                  android:theme="@style/AppTheme">
        </activity>
    </application>

</manifest>